CREATE TABLE OGGETTICELESTI(
ID_OGGETTO INTEGER PRIMARY KEY,
NOME VARCHAR(60),
TIPO  VARCHAR(30)
);

CREATE TABLE ARTICOLI(
ID_ARTICOLO INTEGER PRIMARY KEY,
TITOLO VARCHAR(60),
CORPO VARCHAR(255),
NLIKE  INTEGER,
NDISLIKE INTEGER,
OGGETTO INTEGER,
DATA DATE,
FOREIGN KEY(OGGETTO) REFERENCES OGGETTICELESTI(ID_OGGETTO)
)